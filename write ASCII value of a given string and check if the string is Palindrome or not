#include <iostream>
using namespace std;

int main() {
    
    string s = "naman";
    for(int i = 0; i<5; i++)
    {
        cout<<int(s[i])<<endl;
    }
    
    int i = 0; int j = 4;
    
    while(i<=j)
    {
        if(s[i]==s[j])
            {
                i++; 
                j--;
            }
        else 
            break;
    }
    if(i>j)
        {
        cout<<"It is a palindrome"<<endl;
        return 0;
        }
    else
        cout<<"It is not a palindrome"<<endl;
    
    return 0;

}
